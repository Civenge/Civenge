fizzbuzz = 0
while fizzbuzz < 100: #sets maximum for game to count to
    fizzbuzz = fizzbuzz + 1
    if fizzbuzz % 3 == 0 and fizzbuzz % 5 == 0:
        print("FizzBuzz")
        continue
    elif fizzbuzz % 3 == 0:
        print("Fizz")
        continue
    elif fizzbuzz % 5 == 0:
        print("Buzz")
        continue
    print(fizzbuzz)
